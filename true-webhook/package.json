{
    "name": "true-webhook",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": "20.x"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "npm run prisma:generate && next build && tsc -p tsconfig.server.json",
        "start": "cross-env NODE_ENV=production node dist/server.js",
        "start:railway": "node scripts/railway-start.mjs",
        "lint": "next lint",
        "prisma:generate": "prisma generate --config=prisma/prisma.config.ts",
        "prisma:migrate": "prisma migrate dev --config=prisma/prisma.config.ts",
        "prisma:push": "prisma db push --config=prisma/prisma.config.ts"
    },
    "dependencies": {
        "@prisma/client": "^7.2.0",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^5.2.1",
        "next": "15.1.11",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "prisma": "^7.2.0",
        "zod": "^4.3.5"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.6",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "cross-env": "^10.1.0",
        "eslint": "^9",
        "eslint-config-next": "15.1.6",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5"
    }
}