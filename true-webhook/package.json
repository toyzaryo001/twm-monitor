{
    "name": "true-webhook",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": "20.x"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "npm run prisma:generate && next build && tsc -p tsconfig.server.json",
        "start": "node scripts/railway-start.mjs",
        "start:railway": "node scripts/railway-start.mjs",
        "lint": "next lint",
        "prisma:generate": "prisma generate",
        "prisma:migrate": "prisma migrate dev",
        "prisma:push": "prisma db push",
        "postinstall": "prisma generate"
    },
    "dependencies": {
        "@prisma/client": "^5.22.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.0",
        "next": "15.1.11",
        "node-fetch": "^2.7.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20",
        "@types/node-fetch": "^2.6.13",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "cross-env": "^7.0.3",
        "eslint": "^8",
        "eslint-config-next": "15.1.6",
        "prisma": "^5.22.0",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5"
    }
}
