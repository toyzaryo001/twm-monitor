{
  "name": "true-webhook",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "next build && tsc -p tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "start:railway": "prisma migrate deploy && cross-env NODE_ENV=production node dist/server.js",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "^7.2.0",
    "cors": "^2.8.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "next": "15.1.6",
    "eslint-config-next": "15.1.6",
    "react-dom": "18.2.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "prisma": "^7.2.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5"
  }
}
